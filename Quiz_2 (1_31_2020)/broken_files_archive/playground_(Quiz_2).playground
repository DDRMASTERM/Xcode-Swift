import Foundation

var s="Hello_World"

let range = s.index(s.endIndex, offsetBy: -6) ..< s.endIndex
print(s[range])
s.removeSubrange(range)
print(s)
print(s[s.index(s.endIndex, offsetBy: -2)])
var y=s[s.index(s.endIndex, offsetBy: -1)]
print(y)
